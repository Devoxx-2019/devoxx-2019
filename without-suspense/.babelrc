{
  "presets": [
    [
      "env",
      {
        "include": ["styled-components"]
      }
    ],
    "stage-0",
    "react"
  ],
  "plugins": [
    [
      "styled-components",
      "transform-runtime",
      {
        "polyfill": false,
        "regenerator": true
      }
    ],
    "transform-class-properties"
  ]
}
